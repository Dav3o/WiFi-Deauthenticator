{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "strict": true,
        "esModuleInterop": true,
        "sourceMap": true,
        "outDir": "./build",
        "baseUrl": "./src",
        "suppressImplicitAnyIndexErrors": true
    },
    "include": [
        "src/*"
    ],
    "exclude": [
        "node_modules",
        "**/*.spec.ts"
    ]
}